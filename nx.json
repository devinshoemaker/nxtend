{
  "npmScope": "nxtend",
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "projects": {
    "ionic-react": {
      "tags": []
    },
    "ionic-react-e2e": {
      "tags": [],
      "implicitDependencies": ["ionic-react"]
    },
    "docs": {
      "tags": []
    },
    "capacitor": {
      "tags": []
    },
    "capacitor-e2e": {
      "tags": [],
      "implicitDependencies": ["capacitor"]
    },
    "ionic-angular": {
      "tags": []
    },
    "ionic-angular-e2e": {
      "tags": [],
      "implicitDependencies": ["ionic-angular"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "accessToken": "",
        "cacheableOperations": ["build", "test", "lint", "e2e"],
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  }
}
